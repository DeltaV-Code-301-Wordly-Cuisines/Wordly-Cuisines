<!DOCTYPE html>
<html lang="en-us">
  <head>
    <%- include ('../../partials/head') %>
  </head>
<body>
  <header id="header">
    <%- include ('../../partials/header') %>
  </header>
  
    <main id="main-content">
    <section>
        <% recipes.forEach(recipe => {%>
            <img src="<%= recipe.image %>"/>
            <p><%= recipe.recipeName %></p>
            <p><%= recipe.cuisineType %></p>
            <p><%= recipe.yield %></p>
            <p><%= recipe.url %></p>
            <% }) %>
    </section>
  </main>
  </body>
  <footer>
    <%- include ('../../partials/footer') %>
  </footer>
  
   
  <footer id="footer">   
    <%- include ('../../partials/footer') %>
  </footer>
</html>